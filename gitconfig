# DO NOT EDIT BELOW THIS LINE
[core]
	eol = lf
	autocrlf = false
	excludesfile = ~/.cvsignore
[color]
	ui = always
	status = always
	diff = always
	branch = always
	interactive = always
	grep = always
[alias]
	ci = commit
	cia = commit -a
	co = checkout
	stat = status
	st = status --short
	cinum = !git log --pretty=oneline | wc -l | sed -e \"s/ //g\"
